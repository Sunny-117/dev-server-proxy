{
  "name": "dev-server-proxy-monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm --filter dev-server-proxy build",
    "dev": "pnpm --filter dev-server-proxy dev",
    "clean": "pnpm -r run clean",
    "playground": "pnpm --filter playground dev",
    "lint": "oxlint packages/core/src",
    "format": "oxlint --fix packages/core/src && oxfmt packages/core/src",
    "format:check": "oxlint --deny-warnings packages/core/src"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "oxfmt": "^0.15.0",
    "oxlint": "^1.30.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
