{
  "name": "playground",
  "version": "1.0.0",
  "private": true,
  "description": "Playground for testing dev-server-proxy",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.config.js",
    "start": "webpack serve --config webpack.dev.config.js",
    "start:mock": "PROXY=mock webpack serve --config webpack.dev.config.js",
    "start:test": "PROXY=test webpack serve --config webpack.dev.config.js",
    "start:online": "PROXY=online webpack serve --config webpack.dev.config.js",
    "dev:rspack": "rspack serve --config rspack.dev.config.js",
    "start:rspack": "rspack serve --config rspack.dev.config.js",
    "start:rspack:mock": "PROXY=mock rspack serve --config rspack.dev.config.js",
    "start:rspack:test": "PROXY=test rspack serve --config rspack.dev.config.js",
    "start:rspack:online": "PROXY=online rspack serve --config rspack.dev.config.js"
  },
  "dependencies": {
    "dev-server-proxy": "workspace:*",
    "chalk": "^2.4.2",
    "request": "^2.88.0",
    "lz-string": "^1.4.4",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.5.3",
    "@rspack/cli": "^1.1.7",
    "@rspack/core": "^1.1.7"
  }
}
